{
    "compilerOptions": {
      "target": "es5",
      "lib": ["es5", "dom"],
      "types": ["cypress", "node"], 
      "moduleResolution": "node",
      "isolatedModules": true,
      "noEmit": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["**/*.ts", "cypress/**/*.ts"]
  }
